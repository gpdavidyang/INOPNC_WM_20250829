{
  "name": "inopnc-wm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "dev:fast": "NEXT_TELEMETRY_DISABLED=1 next dev --port 3000",
    "build": "NEXT_BUILD_MODE=true NEXT_PUBLIC_DISABLE_FETCH_LOGS=true NEXT_PUBLIC_DISABLE_CONSOLE_LOGS=true next build",
    "build:vercel": "NEXT_BUILD_MODE=true NEXT_PUBLIC_DISABLE_FETCH_LOGS=true NEXT_PUBLIC_DISABLE_CONSOLE_LOGS=true next build",
    "start": "next start",
    "lint": "next lint --dir app/api/admin --dir app/dashboard/admin --dir components/admin --dir lib/admin --dir lib/auth --file middleware.ts --dir app/actions/shared --dir app/api/auth/me",
    "lint:full": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:critical": "jest --testNamePattern='(Authentication|Supabase|Middleware)' --bail",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run type-check && npm run lint && npm test && npm run test:e2e",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "clean:logs": "chmod +x ./scripts/clean-logs.sh && ./scripts/clean-logs.sh",
    "build:prod": "npm run clean:logs && NODE_ENV=production next build",
    "start:prod": "NODE_ENV=production next start",
    "migrate:documents": "tsx scripts/migrate-documents.ts",
    "migrate:documents:dry": "tsx scripts/migrate-documents.ts --dry-run --verbose",
    "migrate:documents:prod": "tsx scripts/migrate-documents.ts --env=production --backup",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "postcss": "^8",
    "autoprefixer": "^10.4.21",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/nextjs": "^10.7.0",
    "@sparticuz/chromium": "^138.0.2",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.53.0",
    "@tanstack/react-query": "^5.87.1",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.14",
    "@types/pdfkit": "^0.17.2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "canvas": "^3.2.0",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "cron": "^4.3.3",
    "date-fns": "^4.1.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.2",
    "jspdf-autotable": "^5.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.534.0",
    "next": "14.2.3",
    "next-themes": "^0.4.6",
    "pdfkit": "^0.17.2",
    "puppeteer-core": "^24.18.0",
    "react": "^18",
    "react-chartjs-2": "^5.3.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "^18",
    "react-error-boundary": "^6.0.0",
    "recharts": "^3.2.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.20.3",
    "web-push": "^3.6.7",
    "web-vitals": "^5.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@cyanheads/git-mcp-server": "^2.3.2",
    "@faker-js/faker": "^10.0.0",
    "@modelcontextprotocol/server-github": "^2025.4.8",
    "@playwright/test": "^1.55.0",
    "@supabase/mcp-server-supabase": "^0.5.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "babel-jest": "^30.1.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^10.1.8",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "lint-staged": "^16.1.6",
    "node-fetch": "^3.3.2",
    "prettier": "^3.6.2",
    "puppeteer": "^24.20.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
