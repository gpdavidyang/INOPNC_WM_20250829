# Build-time Environment Variables
# These are used during the build process to control logging and behavior

# Disable authentication bypass in builds
NEXT_PUBLIC_DEV_AUTH_BYPASS=false

# Suppress fetch logging in builds
NEXT_PUBLIC_DISABLE_FETCH_LOGS=true

# Disable console logs during build
NEXT_PUBLIC_DISABLE_CONSOLE_LOGS=true

# Build mode indicator
NEXT_BUILD_MODE=true

# Disable telemetry
NEXT_TELEMETRY_DISABLED=1