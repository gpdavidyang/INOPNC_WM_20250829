{
  "buildCommand": "NEXT_BUILD_MODE=true NEXT_PUBLIC_DISABLE_FETCH_LOGS=true NEXT_PUBLIC_DISABLE_CONSOLE_LOGS=true rm -rf .next && next build",
  "framework": "nextjs",
  "regions": ["icn1"],
  "installCommand": "rm -rf node_modules package-lock.json && npm install --legacy-peer-deps",
  "env": {
    "NEXT_PUBLIC_DEV_AUTH_BYPASS": "false",
    "NEXT_BUILD_MODE": "true",
    "NEXT_PUBLIC_DISABLE_FETCH_LOGS": "true",
    "NEXT_PUBLIC_DISABLE_CONSOLE_LOGS": "true"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_DEV_AUTH_BYPASS": "false",
      "NEXT_BUILD_MODE": "true",
      "NEXT_PUBLIC_DISABLE_FETCH_LOGS": "true",
      "NEXT_PUBLIC_DISABLE_CONSOLE_LOGS": "true",
      "NODE_ENV": "production"
    }
  },
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        },
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ],
  "functions": {
    "app/api/shared-documents/route.ts": {
      "maxDuration": 30
    },
    "app/api/upload-handler/route.ts": {
      "maxDuration": 60
    },
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "crons": [
    {
      "path": "/api/cron/daily-reminder",
      "schedule": "0 7 * * *"
    }
  ]
}
